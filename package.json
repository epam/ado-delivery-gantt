{
  "name": "ado-planner-plugin",
  "description": "Use for extend deliverty plan",
  "author": "EPAM",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./out",
    "lint": "eslint --fix .",
    "env": "cross-env NODE_ENV=production PUBLISHER_NAME=EPAM EXTENSION_NAME= EXTENSION_VERSION=0.2.6",
    "compile": "npm run clean && npm run env -- webpack",
    "package": "npm run compile && npm run env -- tfx extension create --manifest-js extension.config.js --output-path out",
    "publish": "npm run compile && npm run env -- tfx extension publish --manifest-js extension.config.js --output-path out --token",
    "start:dev": "npm run env -- webpack-dev-server --mode development"
  },
  "dependencies": {
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "file:./libs/azure-devops-ui.tar.gz",
    "gantt-task-react": "0.3.9",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "abortcontroller-polyfill": "1.7.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.48.1",
    "@webpack-cli/generators": "^3.0.1",
    "mini-css-extract-plugin": "^2.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tfx-cli": "^0.12.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "cross-env": "^7.0.3"
  }
}
