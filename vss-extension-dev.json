{
  "manifestVersion": 1,
  "version": "1.0.1",
  "name": "[Dev] Delivery Gantt",
  "description": "Effortlessly track team progress with a hierarchical Gantt chart displaying Epics, Features, and Stories issue types.",
  "publisher": "{INSERT_PUBLISHER}",
  "demands": [
    "api-version/3.0"
  ],
  "categories": [
    "Azure Boards"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "img/calendar_light.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "ado-planner-plugin",
      "type": "ms.vss-web.hub",
      "description": "ADO Planner plugin",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "[Dev] Delivery Gantt",
        "icon": {
          "dark": "img/calendar_dark.png",
          "light": "img/calendar_light.png"
        },
        "uri": "dist/hub.html"
      }
    }
  ]
}