{
    "version": "0.1.0",
    "configurations": [
      {
        "name": "Launch Firefox",
        "type": "firefox",
        "request": "attach",
        "url": "https://localhost:3000/dist/hub.html",
        "pathMappings": [
          {
            "url": "webpack://ado-planner-plugin/src",
            "path": "${workspaceFolder}/src"
          }
        ]
      },
      {
        "name": "Launch Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "https://localhost:3000/dist/hub.html",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "trace": true,
        "pathMappings": [
          {
            "url": "webpack://ado-planner-plugin/src",
            "path": "${workspaceFolder}/src"
          }
        ]
      },
      {
        "name": "Attach to Chrome",
        "type": "chrome",
        "request": "attach",
        "url": "https://localhost:3000/dist/hub.html",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "trace": true,
        "pathMappings": [
          {
            "url": "webpack://ado-planner-plugin/src",
            "path": "${workspaceFolder}/src"
          }
        ]
      }
    ]
  }